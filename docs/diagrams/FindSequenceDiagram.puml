@startuml
!include style.puml


box Logic LOGIC_COLOR_T1
participant ":LogicManager" as LogicManager LOGIC_COLOR
participant "cmd:FindCommand" as FindCommand LOGIC_COLOR
end box
box Model MODEL_COLOR_T1
participant ":Model" as Model MODEL_COLOR
participant ":FilteredList" as FilteredList MODEL_COLOR
participant ":TaskList" as TaskList MODEL_COLOR
end box

[-> LogicManager : execute("find task 1")
activate LogicManager

LogicManager -> FindCommand : execute()
activate FindCommand

FindCommand -> Model: updateFilteredTaskList\n(predicate)
activate Model

Model -> FilteredList: setPredicate(predicate)
activate FilteredList
ref over FilteredList: use predicate's test\n to get edit distance
FilteredList -> Model
deactivate FilteredList

Model -> FindCommand
deactivate Model

FindCommand -> Model: sortSearchByRelevance\n(searchComparator)
activate Model

opt this.comparator exists
    Model -> Model: searchComparator.thenComparing(this.comparator)
end

Model -> TaskList:sort(searchComparator)
activate TaskList
TaskList -[hidden]> Model
TaskList -> Model
deactivate TaskList


Model -> FindCommand
deactivate Model

FindCommand -> LogicManager: commandResult
deactivate FindCommand

[<--LogicManager
deactivate LogicManager
@enduml

@startuml
!include style.puml

box Sort LOGIC_COLOR_T1
participant ":LogicManager" as LogicManager LOGIC_COLOR
participant "s:SortCommand" as SortCommand LOGIC_COLOR
participant ":CommandResult" as CommandResult LOGIC_COLOR
end box

box Model MODEL_COLOR_T1
participant ":Model" as Model MODEL_COLOR
participant "tl:TaskList" as TaskList MODEL_COLOR
end box

[-> LogicManager : execute("sort priority name")
activate LogicManager

ref over LogicManager
    Similar to DeleteCommand's way
    of generating {action}Command
end ref



LogicManager -> SortCommand : execute()
activate SortCommand

SortCommand -> Model : setComparator(Comparator[])
activate Model

Model -> Model: sortTaskList()
activate Model
loop [int 0..n]
    Model -> TaskList: sort(Comparator)
    activate TaskList
    TaskList -> Model
    deactivate TaskList
end
deactivate Model
Model <- Model

Model -> SortCommand
deactivate Model


create CommandResult
SortCommand -> CommandResult
activate CommandResult

CommandResult --> SortCommand
deactivate CommandResult

SortCommand --> LogicManager : result
deactivate SortCommand

[<--LogicManager
deactivate LogicManager
@enduml
